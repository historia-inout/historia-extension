{
  "manifest_version": 2,
  "name": "Historia",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content.js"]
    }
  ],
  "background": {
    "scripts":["/jquery.js","/background.js"]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "history"
  ]
}
